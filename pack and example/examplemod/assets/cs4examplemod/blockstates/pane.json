{
  "multipart": [
    {
      "when": {"subtype": "subtype0"},
      "apply": {"model": "glass_pane_post"}
    },
    {
      "when": {"north": true, "subtype": "subtype0"},
      "apply": {"model": "glass_pane_side"}
    },
    {
      "when": {"east": true, "subtype": "subtype0"},
      "apply": {"model": "glass_pane_side", "y": 90}
    },
    {
      "when": {"south": true, "subtype": "subtype0"},
      "apply": {"model": "glass_pane_side_alt"}
    },
    {
      "when": {"west": true, "subtype": "subtype0"},
      "apply": {"model": "glass_pane_side_alt", "y": 90}
    },
    {
      "when": {"north": false, "subtype": "subtype0"},
      "apply": {"model": "glass_pane_noside"}
    },
    {
      "when": {"east": false, "subtype": "subtype0"},
      "apply": {"model": "glass_pane_noside_alt"}
    },
    {
      "when": {"south": false, "subtype": "subtype0"},
      "apply": {"model": "glass_pane_noside_alt", "y": 90}
    },
    {
      "when": {"west": false, "subtype": "subtype0"},
      "apply": {"model": "glass_pane_noside", "y": 270}
    },
    {
      "when": {"subtype": "subtype1"},
      "apply": {"model": "iron_bars_post_ends"}
    },
    {
      "when": {"north": false, "east": false, "south": false, "west": false, "subtype": "subtype1"},
      "apply": {"model": "iron_bars_post"}
    },
    {
      "when": {"north": true, "east": false, "south": false, "west": false, "subtype": "subtype1"},
      "apply": {"model": "iron_bars_cap"}
    },
    {
      "when": {"north": false, "east": true, "south": false, "west": false, "subtype": "subtype1"},
      "apply": {"model": "iron_bars_cap", "y": 90}
    },
    {
      "when": {"north": false, "east": false, "south": true, "west": false, "subtype": "subtype1"},
      "apply": {"model": "iron_bars_cap_alt"}
    },
    {
      "when": {"north": false, "east": false, "south": false, "west": true, "subtype": "subtype1"},
      "apply": {"model": "iron_bars_cap_alt", "y": 90}
    },
    {
      "when": {"north": true, "subtype": "subtype1"},
      "apply": {"model": "iron_bars_side"}
    },
    {
      "when": {"east": true, "subtype": "subtype1"},
      "apply": {"model": "iron_bars_side", "y": 90}
    },
    {
      "when": {"south": true, "subtype": "subtype1"},
      "apply": {"model": "iron_bars_side_alt"}
    },
    {
      "when": {"west": true, "subtype": "subtype1"},
      "apply": {"model": "iron_bars_side_alt", "y": 90}
    }
  ]
}
